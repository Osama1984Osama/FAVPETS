</div>\n        </div>\n      </div>\n      \n      <div className=\" main-home\">\n      \n\n        {product &&\n          product.map((item) => {\n            const detailsLink = `/details/${item._id}`;\n            return (\n              <>\n                <Link to={detailsLink}>\n                  <div className=\"card-item\">\n                    <p className='img-para'>\n                      {item.photo ? (\n                        <img style={{ height: '350px', }} src={`/uploads/${item.photo}`}\n                        />\n                      ) : (\n                          \"no photo \"\n                        )}\n                    </p>\n                    \n                    <h3> Name: {item.name}</h3>\n                    <hr></hr>\n                    <h3>Price: {item.price}</h3>\n                    <hr></hr>\n                    <h3> Description: {item.description}</h3>\n                    <div className=\"image-item\"></div>\n                    <hr></hr>\n                    <h3>Location: {item.location}</h3>\n                    <hr></hr>\n                    \n                  </div>\n                </Link>\n                \n              </>\n            );\n          })}\n\n      </div>\n       <footer className=\"footer-home\"> &copy;  All right reserved <br></br> Set by Osama, Mahmoud & Zaher &reg; </footer >\n      \n      \n    </>\n  );\n}\nexport default Home;\n","/home/osama/projects/favpets-Z-12.02/favpets-Z/front_end/src/components/main-navbar/Register.js",[],"/home/osama/projects/favpets-Z-12.02/favpets-Z/front_end/src/components/main-navbar/About.js",["182","183","184","185","186","187","188","189","190","191","192","193"],"import React from 'react'\nimport './about.css'\nfunction About() {\n  return (\n    <div className = \"main-info\">\n\n      <div className=\"info\">\n        <div>\n          <img className='avatar' src='/img/ZAHER.jpg' />\n          <h1>ZAHER</h1>\n          <p className=\"info-text\">\n          